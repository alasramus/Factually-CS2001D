generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model UserInfo {
  id        String @id
  firstName String
  lastName  String
  email     String @unique
  password  String
}

model Achievements {
  id     String  @id
  achv_1 Boolean
  achv_2 Boolean
  achv_3 Boolean
  achv_4 Boolean
  level  Int
  uid    String  @unique
}

model Activities {
  id       String @id
  topic    String
  activity String
  desc     String
}
